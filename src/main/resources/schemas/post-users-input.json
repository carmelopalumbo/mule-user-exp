{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Users",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "address": {
        "type": "string"
      }
    },
    "required": ["email"],
    "additionalProperties": false
  },
  "additionalItems": false
}
